package Testonline7;

import java.util.Scanner;

public class productofcubeofeachdigit {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number");
		int num=sc.nextInt();
		int product=1, cube=0;
		while(num>0) {
			int n=num%10;
			cube=n*n*n;
			System.out.println("Cube "+cube);
			product=product*cube;
			System.out.println("Peoduct "+product);
			num=num/10;
			System.out.println();
		}
		
	}

}
